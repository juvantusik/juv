// @ts-check
// BEGIN (write your solution here)

const finalGrade = (exam, project) => {
  if ((exam > 90 && exam <= 100) || (project > 10)) {
    return 100;
  }
  if ((exam > 75 && exam <= 90) && (project >= 5 && project <= 10)) {
    return 90;
  }
  if ((exam > 50 && exam < 75) && (project >= 2 && project < 5)) {
    return 75;
  }
  return 0;
};
// END

export default finalGrade;
